
<form action="#" method="get" class="col s10 offset-s1 hide-form" id="add-member-form" data-id="<%=@project.id%>">
  <input type="hidden" value="<%= form_authenticity_token() %>" name="authenticity_token"/>
  <div class="row">
    <div class="row">
        <%= label_tag "Our People"%>
    </div>
    <div class="row" id="membersList">      
      <%= collection_check_boxes(:member,:id,Member.where(project_id: nil),:id,:name) do |b|%>  
        <div class="col s12 m6 l4">
        <%= b.check_box %>
        <%= b.label %>
      </div>
      <% end %>
    </div>
    <div class="row">
      <div class="col s12 center-align">
        <%= link_to '#', class: "btn-floating", id: "add-member-button" do %> 
          <i class="fa fa-plus small"></i>
        <% end %>
      </div>      
    </div>    
  </div>
</form>

  

