<form action="#" method="get" class="col s10 offset-s1 hide-form" id="add-skill-form" data-id="<%=@project.id%>">
  <input type="hidden" value="<%= form_authenticity_token() %>" name="authenticity_token"/>
  <div class="row">
      <%= label_tag "Our Skill Set"%>
  </div>
  <div class="row" id="skillsList">      
    <%= collection_check_boxes(:skill,:id,@skills_left,:id,:name) do |b|%>  
      <div class="col s12 m6 l4">
      <%= b.check_box %>
      <%= b.label %>
    </div>
    <% end %>
  </div>
  <div class="row">
    <div class="col s12 center-align">
      <%= link_to '#', class: "btn-floating", id: "add-skill-button" do %> 
        <i class="fa fa-plus small"></i>
      <% end %>
    </div>      
  </div>
</form>